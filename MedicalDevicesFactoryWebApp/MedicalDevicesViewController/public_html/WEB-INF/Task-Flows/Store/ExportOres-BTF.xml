<?xml version="1.0" encoding="windows-1256" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ExportOres-BTF">
    <default-activity>ExportOres</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="ExportOres">
      <page>/pages/Store/ExportOres.jsff</page>
    </view>
    <task-flow-return id="Back">
      <outcome>
        <name>Back</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>ExportOres</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Back</from-outcome>
        <to-activity-id>Back</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
